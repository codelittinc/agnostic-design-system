{
  "name": "@codelitt/ay-design-library",
  "version": "0.1.0",
  "description": "React components for AY related projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev:start": "npx styleguidist server",
    "prod:build": "rm -rf ./dist && NODE_ENV=production webpack --mode production",
    "docker:dev": "sh scripts/dev.sh",
    "docs:prod:build": "npx styleguidist build",
    "docs:docker:build": "sh scripts/buildDocs.sh",
    "docs:docker:tag": "sh scripts/dockerTagDocs.sh",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' && stylelint 'src/**/*.{css,scss}'",
    "lint:fix": "tsc --noEmit && eslint --fix '*/**/*.{js,ts,tsx}' && stylelint --fix 'src/**/*.{css,scss}'",
    "prettier:format": "prettier --write \"src/**/*.+(ts|tsx|json|css)\" \"src/**/*.test.ts\"",
    "test": "jest",
    "test:ci": "jest --reporters=jest-junit",
    "test:updateSnapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codelittinc/ay-design-library.git"
  },
  "author": "Codelitt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codelittinc/ay-design-library/issues"
  },
  "homepage": "https://github.com/codelittinc/ay-design-library#readme",
  "dependencies": {
    "@codelitt/ay-css-library": "^0.1.1-1",
    "classnames": "^2.2.6",
    "identity-obj-proxy": "^3.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@svgr/webpack": "^4.3.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.1",
    "@types/classnames": "^2.2.9",
    "@types/eslint": "^6.1.3",
    "@types/jest": "^24.0.21",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-test-renderer": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "babel-jest": "^24.9.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "jest-transform-stub": "^2.0.0",
    "node-sass": "^4.13.0",
    "postcss": "^7.0.21",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "react-docgen-typescript": "^1.15.1",
    "react-styleguidist": "^10.0.1",
    "react-test-renderer": "^16.11.0",
    "release-it": "^12.4.3",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-css-modules": "^1.5.0",
    "stylelint-config-sass-guidelines": "^6.1.0",
    "stylelint-config-standard": "^19.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.6.4",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "eslintConfig": {
    "extends": "[react-app, plugin:prettier/recommended]"
  },
  "release": {
    "publishDir": "dist"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:format && npm run lint && npm run test"
    }
  }
}
